<%- include("partials/header") -%>
<h1>Compose</h1>
<form action="/compose" method="POST">
  <div class="form-group">
    <label>Title</label>
    <input type="text" name="postTitle" size="40" class="form-control" />
    <label>Post</label>
    <textarea
      name="postBody"
      cols="30"
      rows="5"
      class="form-control"
    ></textarea>
  </div>
  <button type="submit" name="button" value="publish" class="btn btn-primary">
    Publish
  </button>
</form>
<%- include("partials/footer") -%>
